{
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "rabbitmqhost"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "rabbitmqhost",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "queues": [
    {
      "name": "devices_all_returned",
      "vhost": "rabbitmqhost",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-queue-type": "classic" }
    },
    {
      "name": "employment_terminated_devices",
      "vhost": "rabbitmqhost",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-queue-type": "classic" }
    },
    {
      "name": "employment_terminated_permissions",
      "vhost": "rabbitmqhost",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-queue-type": "classic" }
    },
    {
      "name": "permissions_offboarding_status",
      "vhost": "rabbitmqhost",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-queue-type": "classic" }
    }
  ],
  "exchanges": [
    {
      "name": "devices.events",
      "vhost": "rabbitmqhost",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "hr.events",
      "vhost": "rabbitmqhost",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    },
    {
      "name": "permissions.events",
      "vhost": "rabbitmqhost",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "devices.events",
      "vhost": "rabbitmqhost",
      "destination": "devices_all_returned",
      "destination_type": "queue",
      "routing_key": "devices.returned",
      "arguments": {}
    },
    {
      "source": "hr.events",
      "vhost": "rabbitmqhost",
      "destination": "employment_terminated_devices",
      "destination_type": "queue",
      "routing_key": "employment.terminated",
      "arguments": {}
    },
    {
      "source": "hr.events",
      "vhost": "rabbitmqhost",
      "destination": "employment_terminated_permissions",
      "destination_type": "queue",
      "routing_key": "employment.terminated",
      "arguments": {}
    },
    {
      "source": "permissions.events",
      "vhost": "rabbitmqhost",
      "destination": "permissions_offboarding_status",
      "destination_type": "queue",
      "routing_key": "offboarding.status",
      "arguments": {}
    }
  ]
}
