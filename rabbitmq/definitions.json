{
  "users": [
    {
      "name": "guest",
      "password": "guest",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "rabbitmqhost"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "rabbitmqhost",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "queues": [
    {
      "name": "lagerqueue_einlagerung",
      "vhost": "rabbitmqhost",
      "durable": true,
      "auto_delete": false,
      "arguments": { "x-queue-type": "classic" }
    }
  ],
  "exchanges": [
    {
      "name": "lager.events",
      "vhost": "rabbitmqhost",
      "type": "direct",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "lager.events",
      "vhost": "rabbitmqhost",
      "destination": "lagerqueue_einlagerung",
      "destination_type": "queue",
      "routing_key": "artikel.eingetroffen",
      "arguments": {}
    }
  ]
}
